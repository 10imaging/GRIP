<fx:root type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/null"
         xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <TitledPane fx:id="stepPane" alignment="CENTER" animated="false" collapsible="false" maxHeight="-Infinity"
                    maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" styleClass="step">
            <graphic>
                <HBox styleClass="icons">
                    <children>
                        <Button fx:id="deleteButton" text="âœ•">
                            <HBox.margin>
                                <Insets/>
                            </HBox.margin>
                        </Button>
                        <ImageView fx:id="icon"/>
                    </children>
                </HBox>
            </graphic>
            <content>
                <VBox styleClass="sockets">
                    <children>
                        <VBox fx:id="inputs"/>
                        <Separator orientation="HORIZONTAL"/>
                        <VBox fx:id="outputs"/>
                    </children>
                </VBox>
            </content>
        </TitledPane>
    </children>
    <stylesheets>
        <URL value="@roboto/Roboto.css"/>
        <URL value="@GRIP.css"/>
    </stylesheets>
</fx:root>
